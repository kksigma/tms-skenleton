{
    "name": "kksigma/tms",
    "description": "This is my package TMS",
    "keywords": [
        "kksigma",
        "laravel",
        "tms"
    ],
    "homepage": "https://github.com/kksigma/tms",
    "license": "MIT",
    "authors": [
        {
            "name": "kksigma",
            "email": "kksigma8@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Kksigma\\TMS\\": "src",
            "Kksigma\\TMS\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kksigma\\TMS\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kksigma\\TMS\\TMSServiceProvider"
            ],
            "aliases": {
                "TMS": "Kksigma\\TMS\\TMSFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
